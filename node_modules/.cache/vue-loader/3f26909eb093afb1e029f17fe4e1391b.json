\"\n  }\n];\n\n/*index.addObjects(items, (err, content) => {\n  console.log(content);\n});*/\nexport default {\n  http: {\n    root: '/root',\n    headers: {\n      Authorization: 'Basic YXBpOnBhc3N3b3Jk'\n    }\n  },\n  components: {},\n  data:{\n    parties: []\n  },\n  data: function(){\n    return {\n      parties: [],\n      loading: false\n    }\n  },\n  created: function(){\n    this.sendParties();\n  },\n  methods: {\n    getParties: function() {\n      this.loading = true;\n      console.log(\"fdsfsdf\");\n      this.$http\n        .get(\"https://linkenpartydjango.azurewebsites.net/polls/User/\")\n        .then(response => {\n          this.parties = response.data;\n          this.loading = false;\n        })\n        .catch(err => {\n          this.loading = false;\n          console.log(err);\n        });\n    },\n    sendParties: function(){\n      for(var i=0;i<items.length;i++){\n        this.$store.commit(\"addParty\", items[i]);\n        //this.$store.commit(\"addAdress\", items[i].adress);\n      }\n    }\n  }\n};\n</script>\n\n<style>\n.list-party {\n  overflow-x: auto;\n  display: -webkit-inline-box;\n  padding-top: 25px;\n  padding-bottom: 25px;\n}\n</style>\n\n"]}]}